{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Enumerations from Title 24",
    "description": "Schema addition for ASHRAE 229 related to enumerations related to California Title 24 publications.",
    "definitions": {
        "RulesetModelOptions2019T24Com": {
            "type": "string",
            "enum": [
                "USER",
                "PROPOSED_SIZING",
                "BASELINE_SIZING",
                "PROPOSED",
                "BASELINE"
            ],
            "descriptions": [
                "The building model as described without consideration of the ruleset",
                "The proposed building model used for sizing",
                "The baseline building model used for sizing",
                "The proposed building model",
                "The baseline building model"
            ]
        },
        "RulesetModelOptions2019T24Res": {
            "type": "string",
            "enum": [
                "USER",
                "PROPOSED",
                "STANDARD",
                "BASELINE"
            ],
            "descriptions": [
                "The building model as described without consideration of the ruleset",
                "The proposed building model",
                "The standard building model",
                "The baseline building model"
            ]
        },
        "GeneralStatusOptions2019T24": {
            "type": "string",
            "enum": [
                "NEW",
                "ALTERED",
                "EXISTING",
                "EXISTING_PLUS_NEW"
            ],
            "descriptions": [
                "New",
                "Altered",
                "Existing",
                "Existing plus new"
            ]
        },
        "ServiceWaterHeatingDistributionCompactnessOptions2019T24Com": {
            "type": "string",
            "enum": [
                "NOT_COMPACT",
                "BASIC_COMPACT",
                "EXPANDED_CREDIT",
                "OTHER"
            ],
            "descriptions": [
                "Not compact",
                "Basic compact",
                "Expanded credit",
                "Other"
            ]
        },
        "ServiceWaterHeatingControlOptions2019T24Com": {
            "type": "string",
            "enum": [
                "DEMAND_CONTROL",
                "NO_CONTROL",
                "NO_LOOP_OR_CENTRAL_SYSTEM_PUMP",
                "TEMPERATURE_MODULATION",
                "TEMPERATURE_MODULATION_AND_MONITORING",
                "OTHER"
            ],
            "descriptions": [
                "Demand control",
                "No control",
                "No loop or central system pump",
                "Temperature modulation",
                "Temperature modulation and monitoring",
                "Other"
            ]
        },
        "OutputSchemaOptions2019T24": {
            "type": "string",
            "enum": [
                "OUTPUT_SCHEMA_T24_RESIDENTIAL",
                "OUTPUT_SCHEMA_T24_COMMERCIAL",
                "OTHER"
            ],
            "descriptions": [
                "Output schema for Title 24 Residential",
                "Output schema for Title 24 Commercial",
                "other"
            ]
        }
    }
}